<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="reduce.project.yaerei.suitouchou">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>


    <application
        android:allowBackup="true"
        android:name="com.activeandroid.app.Application"
        android:icon="@drawable/suitouchou"
        android:label="@string/suitouchou"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <meta-data
            android:name="AA_DB_NAME"
            android:value="Sample.db" />
        <meta-data
            android:name="AA_DB_VERSION"
            android:value="1" />

        <meta-data
            android:name="AA_MODELS"
            android:value="reduce.project.yaerei.suitouchou.Item,
            reduce.project.yaerei.suitouchou.peopleItem,
            reduce.project.yaerei.suitouchou.shakkinnItem,
            reduce.project.yaerei.suitouchou.keishouItem,
            reduce.project.yaerei.suitouchou.listtitleItem"/>

        <activity android:name=".MainActivity"
            android:screenOrientation="portrait"
            android:label="@string/main">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".listtitleActivity"
            android:screenOrientation="portrait"
            android:label="@string/mainlisttitle"/>

        <activity android:name=".shakkinnActivity"
            android:screenOrientation="portrait"
            android:label = "@string/shakkinn"/>

        <activity android:name=".passwordActivity"
            android:screenOrientation="portrait"
            android:label = "@string/password"/>

        <activity android:name=".peopleActivity"
            android:screenOrientation="portrait"
            android:label="@string/people"/>

        <activity android:name=".passwordinputActivity"
            android:screenOrientation="portrait"
            android:label="@string/passwordinput"/>

    </application>

</manifest>